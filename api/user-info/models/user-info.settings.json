{
  "kind": "collectionType",
  "collectionName": "user_infos",
  "info": {
    "name": "User Info",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "description": {
      "type": "richtext"
    },
    "currency": {
      "model": "currency"
    },
    "linkedUsers": {
      "plugin": "users-permissions",
      "collection": "user"
    },
    "invites": {
      "collection": "user-info"
    },
    "userId": {
      "type": "string"
    },
    "language": {
      "type": "enumeration",
      "enum": [
        "en",
        "lv"
      ]
    },
    "userCategories": {
      "model": "user-categories"
    }
  }
}
